<Application 
    x:Class="PestControlMapper.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="TabControlStyle.xaml"/>
                <ResourceDictionary Source="TabItemStyle.xaml"/>
                <ResourceDictionary Source="DefaultComboBoxTemplate.xaml"/>
                <ResourceDictionary Source="TextBoxStyle.xaml"/>
                <ResourceDictionary Source="ComboBoxTextBoxStyle.xaml"/>
                <ResourceDictionary Source="ComboBoxItemStyle.xaml"/>
                <ResourceDictionary Source="DefaultButtonStyle.xaml"/>
                <ResourceDictionary Source="DefaultMenuItemStyle.xaml"/>
                <ResourceDictionary Source="MenuStyle.xaml"/>
                <ResourceDictionary Source="MenuItemStyle.xaml"/>
                <ResourceDictionary Source="ScrollbarStyle.xaml"/>
                <ResourceDictionary Source="TreeViewStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
